{
  "name": "@albnnc/matilda",
  "version": "2.0.2",
  "description": "Material Design theme for Bootstrap 4",
  "license": "MIT",
  "author": "albnnc",
  "scripts": {
    "build": "node-sass scss/matilda.scss --importer=node_modules/node-sass-tilde-importer -o dist",
    "start": "cd site && next",
    "predeploy": "cd site && next build && next export -o .dist",
    "deploy": "gh-pages -d site/.dist"
  },
  "dependencies": {
    "bootstrap": "^4.1.2"
  },
  "devDependencies": {
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "diffable-html": "^3.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^2.0.0",
    "history": "^4.7.2",
    "mobx": "^5.1.2",
    "mobx-react": "^5.2.8",
    "mobx-react-router": "^4.0.5",
    "next": "^7.0.0",
    "node-sass": "^4.9.3",
    "node-sass-tilde-importer": "^1.0.2",
    "prettier": "^1.14.3",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-hot-loader": "^4.3.11",
    "react-syntax-highlighter": "^8.0.1",
    "reactstrap": "^6.4.0",
    "stylelint": "^9.5.0",
    "stylelint-config-idiomatic-order": "^5.0.0"
  }
}
